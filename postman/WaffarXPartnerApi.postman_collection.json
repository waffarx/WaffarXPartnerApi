					"const formattedDateTime = dateTime.replaceAll(\"-\", \"\").replaceAll(\":\", \"\");",
					"",
					"const method = pm.request.method.toLocaleLowerCase();",
					"const actionName = `/api/${pm.request.url.toString().split(\"/\").slice(1).join(\"/\").toLocaleLowerCase()}`;",
					"const clientKey = \"Bv0fMtFoUVeRm4OElR57CVCGS4dtbNgJAVdZfwc+ct4kmcY5PzqswCbOm9gH3yi59IaiZPfT4V7LC3xOaDk+ZQ==\";",
					"",
					"console.log(pm.request.url.toString().split(\"/\"))",
					"",
					"const stringToSign = data ? `${method}-${actionName}-${clientKey}-${formattedDateTime}-${hashedpayloadstring}` : `${method}-${actionName}-${clientKey}-${formattedDateTime}`;",
					"const secretKey = CryptoJS.enc.Utf8.parse(\"n6rntfyk0amdu7fjzj5u02xpp8adgv4c9dfngiw01z93v7mi31lq5qg8nzf7yjbopb4wnz5vyyzj82s9ld9evbtg4hrvm3z3sw9it41e3sgfj1k3dn7rrgx6vggd3pxkby86khxkjdcz1kjwz5r4yzvnnxeivsf9th6g5k1ou0vjck1jbcus50hfh7gp3utqy99oh1a9k2nz0xajbpr7rhz2mc3cdqjxskpyo93lmh69vraej4d5dd9h6cic8pi8e!@\");",
					"const payloadHmac = CryptoJS.HmacSHA256(stringToSign, secretKey);",
					"const SignedPayload = payloadHmac.toString();",
					"",
					"pm.globals.set(\"wxc-date\", dateTime);",
